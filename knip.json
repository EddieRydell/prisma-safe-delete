{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "project": ["src/**/*.ts", "tests/**/*.ts"],
  "ignore": [
    "tests/integration/generated/**",
    "tests/e2e/generated/**",
    "tests/e2e-sentinel/generated/**",
    "tests/e2e-none/generated/**",
    "tests/e2e-no-cascade/generated/**",
    "tests/**/prisma.config.ts"
  ],
  "ignoreDependencies": ["dotenv", "@prisma/client", "conventional-changelog-conventionalcommits"],
  "ignoreExportsUsedInFile": true
}
